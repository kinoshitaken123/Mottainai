<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-12'>
      <h2 class="mt-3">会員一覧</h2>
      <table class='table table-hover table-inverse'>
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
          </tr>
        </thead>
      <% @user.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.id %></td>
            <td>
              <%= link_to admin_user_path(user) do %>
               <%= user.last_name %><%= user.first_name %>
              <% end %>
            </td>
            <td><%= user.email %></td>
          </tr>
        </tbody>
      <% end %>
      </table>
    </div>
  </div>
</div>